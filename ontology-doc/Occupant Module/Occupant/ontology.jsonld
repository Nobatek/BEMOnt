  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The building state allows to conextualise and determine the building facilities status. This building state determinaton has been divided into simple building states and complex building states."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {

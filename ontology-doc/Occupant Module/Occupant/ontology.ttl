#################################################################
#    Object Properties
#################################################################

###  http://bemserver.org/occupant#acts
:acts rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      rdfs:comment "Relation to define that an iser acto ober certain device changing their behaviour."@en ;
      rdfs:label "acts"@en .


###  http://bemserver.org/occupant#assignedTo
:assignedTo rdf:type owl:ObjectProperty ;
            owl:inverseOf :isAssignedTo ;
            rdfs:comment "Relation between the context and indoor or outddor factors."@en ;
            rdfs:label "assigned to"@en .


###  http://bemserver.org/occupant#associatedTo
:associatedTo rdf:type owl:ObjectProperty ;
              rdfs:comment "Relation between the context and certain sensor output"@en ;
              rdfs:label "associated to"@en .


###  http://bemserver.org/occupant#changes
:changes rdf:type owl:ObjectProperty ;
         rdfs:comment "The relation to represent the result of an action in terms of changing something inside the buildign (system, sensors, etc)."@en ;
         rdfs:label "changes"@en .


###  http://bemserver.org/occupant#concerns
:concerns rdf:type owl:ObjectProperty ;
          rdfs:comment "Relation focused on depicting the link between the users and the corresponding feedback"@en ;
          rdfs:label "concerns"@en .


###  http://bemserver.org/occupant#hasBehaviour
:hasBehaviour rdf:type owl:ObjectProperty ;
              rdfs:comment "relation focused on indicating that certain user have defined certain behaviour"@en ;
              rdfs:label "has behaviour"@en .


###  http://bemserver.org/occupant#hasComfortRequirements
:hasComfortRequirements rdf:type owl:ObjectProperty ;
                        rdfs:comment "The relation to represent that a user has certain comfort requirements to be satisfied"@en ;
                        rdfs:label "has comfort requirements"@en .


###  http://bemserver.org/occupant#hasConstraints
:hasConstraints rdf:type owl:ObjectProperty ;
                rdfs:comment "the relation to represent that an event have a set of contraints"@en ;
                rdfs:label "has constraints"@en .


###  http://bemserver.org/occupant#hasContext
:hasContext rdf:type owl:ObjectProperty ;
            rdfs:comment "Indicate the relation between an event and certain context"@en ;
            rdfs:label "has context"@en .


###  http://bemserver.org/occupant#hasFunction
:hasFunction rdf:type owl:ObjectProperty ;
             rdfs:comment "Property to define in the Drivers several functions to measure the comfort."@en ;
             rdfs:label "has Function"@en .


###  http://bemserver.org/occupant#hasGender
:hasGender rdf:type owl:ObjectProperty ;
           rdfs:comment "property to associate to the users their corresponding gender"@en ;
           rdfs:label "has gender"@en .


###  http://bemserver.org/occupant#hasIndicator
:hasIndicator rdf:type owl:ObjectProperty ;
              rdfs:label "has indicator"@en ,
                         "property to indicate to the drivers the associations to certain indicators."@en .


###  http://bemserver.org/occupant#hasModelResult
:hasModelResult rdf:type owl:ObjectProperty ;
                rdfs:comment "property to link the drivers with certain model results."@en ;
                rdfs:label "has model result"@en .


###  http://bemserver.org/occupant#hasOccupantAttitude
:hasOccupantAttitude rdf:type owl:ObjectProperty ;
                     rdfs:range :OccupantAttitude ;
                     rdfs:comment "object property to represent the occupant attitude of the building occupants."@en ;
                     rdfs:label "has occupant attitude"@en .


###  http://bemserver.org/occupant#hasOccupantCharacteristics
:hasOccupantCharacteristics rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf owl:topObjectProperty ;
                            rdfs:range :OccupantCharacteristics ;
                            rdfs:comment "property to define the association between users and their daily activities"@en ;
                            rdfs:label "has occuant characteristics"@en .


###  http://bemserver.org/occupant#hasProbability
:hasProbability rdf:type owl:ObjectProperty ;
                rdfs:comment "property to represent the probability of happen certain event"@en ;
                rdfs:label "has probability"@en .


###  http://bemserver.org/occupant#hasRole
:hasRole rdf:type owl:ObjectProperty ;
         rdfs:comment "property to associate the building occupants to certain role"@en ;
         rdfs:label "has role"@en .


###  http://bemserver.org/occupant#hasState
:hasState rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          owl:inverseOf :isStatedBy ;
          rdfs:range :BuildingState ;
          rdfs:comment "property to asign to the building systems certain associated abstract state"@en ;
          rdfs:label "has state"@en .


###  http://bemserver.org/occupant#hasStatus
:hasStatus rdf:type owl:ObjectProperty ;
           rdfs:comment "property to represent the asignment to the driver of certain status"@en ;
           rdfs:label "has status"@en .


###  http://bemserver.org/occupant#isAssignedTo
:isAssignedTo rdf:type owl:ObjectProperty ;
              rdfs:comment "is assigned to"@en .


###  http://bemserver.org/occupant#isAssociatedTo
:isAssociatedTo rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :Event ;
                rdfs:comment "property to asign to the event a sosa:Result"@en ;
                rdfs:label "isAssociatedTo"@en .


###  http://bemserver.org/occupant#isBasedOn
:isBasedOn rdf:type owl:ObjectProperty ;
           rdfs:label "is Based on"@en .


###  http://bemserver.org/occupant#isProvokedBy
:isProvokedBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :provokes ;
              rdfs:label "is provoked by"@en .


###  http://bemserver.org/occupant#isStatedBy
:isStatedBy rdf:type owl:ObjectProperty ;
            rdfs:label "is stated by"@en .


###  http://bemserver.org/occupant#occupies
:occupies rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain :Occupant ;
          rdfs:comment "property to asign to the occupant occupying a certain bot:Zone"@en ;
          rdfs:label "occupies"@en .


###  http://bemserver.org/occupant#performsAction
:performsAction rdf:type owl:ObjectProperty ;
                rdfs:range :Actions ;
                rdfs:label "perform action"@en .


###  http://bemserver.org/occupant#prefers
:prefers rdf:type owl:ObjectProperty ;
         rdfs:comment "property to link user with modalitites"@en ;
         rdfs:label "prefers"@en .


###  http://bemserver.org/occupant#provokes
:provokes rdf:type owl:ObjectProperty ;
          rdfs:label "provokes"@en .


###  http://bemserver.org/occupant#receives
:receives rdf:type owl:ObjectProperty ;
          rdfs:comment "property to represent the action of receiving notification from the system according t certain building anomalies"@en ;
          rdfs:label "receives"@en .


###  http://bemserver.org/occupant#receivesAlerts
:receivesAlerts rdf:type owl:ObjectProperty ;
                rdfs:range :Alert ;
                rdfs:comment "relation to represent the recepcion of alerts to certain users"@en ;
                rdfs:label "receives alerts"@en .


#################################################################
#    Data properties
#################################################################

###  http://bemserver.org/occupant#age
:age rdf:type owl:DatatypeProperty ;
     rdfs:range xsd:double ;
     rdfs:comment "Data property to define the age of certain building occupant"@en ;
     rdfs:label "age"@en .


###  http://bemserver.org/occupant#endWorktime
:endWorktime rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:dateTime ;
             rdfs:comment "proeprty to define the date time of end of wrok"@en ;
             rdfs:label "end of work time"@en .


###  http://bemserver.org/occupant#id
:id rdf:type owl:DatatypeProperty ;
    rdfs:range xsd:string ;
    rdfs:comment "text representation of the ids"@en ;
    rdfs:label "id"@en .


###  http://bemserver.org/occupant#jobType
:jobType rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:string ;
         rdfs:comment "text definition of the job type"@en ;
         rdfs:label "job type"@en .


###  http://bemserver.org/occupant#lifestyle
:lifestyle rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:range xsd:string ;
           rdfs:label "lifestyle"@en .


###  http://bemserver.org/occupant#maxValue
:maxValue rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:range xsd:double ;
          rdfs:comment "maximun value marked as an upper threshold measure"@en ;
          rdfs:label "maximun value"@en .


###  http://bemserver.org/occupant#minValue
:minValue rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:double ;
          rdfs:comment "minimun value marked as the lower limit of the threshold"@en ;
          rdfs:label "minimun value"@en .


###  http://bemserver.org/occupant#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string ;
      rdfs:comment "name of the instances (text field)"@en ;
      rdfs:label "name"@en .


###  http://bemserver.org/occupant#numericValue
:numericValue rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:double ;
              rdfs:label "numeric value"@en .


###  http://bemserver.org/occupant#resultTime
:resultTime rdf:type owl:DatatypeProperty ;
            rdfs:comment "The result time is the instant of time when the Observation, Actuation or Sampling activity was completed." ;
            rdfs:isDefinedBy <http://www.w3.org/ns/sosa/> ;
            rdfs:label "result time" .


###  http://bemserver.org/occupant#startWorktime
:startWorktime rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:range xsd:dateTime ;
               rdfs:comment "property to define the date time of start of wrok"@en ;
               rdfs:label "start of work time"@en .


###  http://bemserver.org/sensor#externalID
<http://bemserver.org/sensor#externalID> rdf:type owl:DatatypeProperty ;
                                         rdfs:range xsd:anyURI ;
                                         rdfs:comment "Data proeprty to define the external ID regaring the time series stored in Mondas"@en ;
                                         rdfs:isDefinedBy "http://bemserver.org/sensor#externalID"@en ;
                                         rdfs:label "externalID"@en .


#################################################################
#    Classes
#################################################################

###  http://bemserver.org/occupant#ASHRAEAdaptiveComfort
:ASHRAEAdaptiveComfort rdf:type owl:Class ;
                       rdfs:subClassOf :ThermalComfort ;
                       rdfs:comment "The ASHRAE Adaptive Comfort is a standard that provides minimum requirements for acceptable thermal indoor environments. It establishes the ranges of indoor environmental conditions that are acceptable to achieve thermal comfort for occupants. It was first published in 1966, and since 2004 has been updated periodically by a technical committee of industry experts in ASHRAE (American Society of Heating, Refrigerating and Air-Conditioning Engineers). The most recent version of the standard was published in 2013."@en ;
                       rdfs:label "ASHRAE Adaptive Comfort"@en .


###  http://bemserver.org/occupant#ASHRAEAdaptiveEnvelope
:ASHRAEAdaptiveEnvelope rdf:type owl:Class ;
                        rdfs:subClassOf :ThermalComfort ;
                        rdfs:comment """Standard that includes prescriptive requirements for the following:
- Building Envelope: minimum wall insulation, minimum roof insulation, roof reflectance, minimum glazing performance
  - HVAC: minimum equipment efficiency, minimum system features, limitation on reheat, limitation on fan power
  - Domestic Hot Water: minimum equipment efficiency, minimum system features
  - Power: transformer efficiency, automatic receptacle controls, energy monitoring
  - Lighting: maximum indoor lighting power density (LPD, expressed in Watts/Sq.Ft.), minimum lighting controls, exterior lighting, parking garage lighting
  - Other Equipment: electric motors, potable water booster pumps, elevators, and escalators"""@en ;
                        rdfs:label "ASHRAE Adaptive Envelope"@en .


###  http://bemserver.org/occupant#AcousticComfort
:AcousticComfort rdf:type owl:Class ;
                 rdfs:subClassOf :Needs ;
                 rdfs:comment "The acoustic comfort refers to the models to measure the level of background noise within an aceptable range."@en ;
                 rdfs:label "Acoustic Comfort"@en .


###  http://bemserver.org/occupant#Actions
:Actions rdf:type owl:Class ;
         rdfs:comment "Actions are the specific building interactions performed by the building occupants. This actions can be caused by an initial discomfort or event as an occupant event. Nevertheless, the actions are connected with the drivers at time as the user changes a parameter in the building facilities. Moreover, the actions also has some influence over the needs due to this actions commonly are performed to satisfy user comfort requirements."@en ;
         rdfs:label "Actions"@en .


###  http://bemserver.org/occupant#ActivityModel
:ActivityModel rdf:type owl:Class ;
               rdfs:subClassOf :Driver ;
               rdfs:comment "Model that consist on modelling the occupant activity inside the building"@en ;
               rdfs:label "Activity Model"@en .


###  http://bemserver.org/occupant#Age
:Age rdf:type owl:Class .


###  http://bemserver.org/occupant#Alert
:Alert rdf:type owl:Class ;
       rdfs:subClassOf :Notification ;
       rdfs:comment "Defined as the quick to notice any unusual and potentially dangerous or difficult circumstances concerning the building state."@en ;
       rdfs:label "Alert"@en .


###  http://bemserver.org/occupant#BehaviourModel
:BehaviourModel rdf:type owl:Class ;
                rdfs:subClassOf :Driver ,
                                [ rdf:type owl:Class ;
                                  owl:unionOf ( [ rdf:type owl:Restriction ;
                                                  owl:onProperty :isBasedOn ;
                                                  owl:someValuesFrom :OccupantBehaviour
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :isBasedOn ;
                                                  owl:someValuesFrom :TimeBehaviour
                                                ]
                                              )
                                ] ;
                rdfs:comment "As variables that serve to measure the habits and activities inside the building. Defined to register and contextualise the occupant events (building interactions with the different systems)."@en ;
                rdfs:label "Behavioural Model"@en .


###  http://bemserver.org/occupant#BlindClose
:BlindClose rdf:type owl:Class ;
            rdfs:subClassOf :BlindState ;
            owl:disjointWith :BlindOpen ;
            rdfs:comment "Determines that the corresponding blind is close"@en ;
            rdfs:label "Blind Close"@en .


###  http://bemserver.org/occupant#BlindOpen
:BlindOpen rdf:type owl:Class ;
           rdfs:subClassOf :BlindState ;
           rdfs:comment "Determines that the corresponding blind is open"@en ;
           rdfs:label "Blind Open"@en .


###  http://bemserver.org/occupant#BlindState
:BlindState rdf:type owl:Class ;
            rdfs:subClassOf :BuildingSimpleState ;
            rdfs:comment "Refers to the state that a blind could achieve (Open or Closed)"@en ;
            rdfs:label "Blind State"@en .


###  http://bemserver.org/occupant#BuildingComplexState
:BuildingComplexState rdf:type owl:Class ;
                      rdfs:subClassOf :BuildingState ;
                      rdfs:comment "In reference to the complex states, this refers as the states that require for the combination of different facilities to determine an overall state. For example, one complex state is the determination of the energy state of the building that can be energy efficient state or energy inefficient state considering anomalous energy consumption (e.g., thermostat on at same time as the windows are open)"@en ;
                      rdfs:label "Building Complex State"@en .


###  http://bemserver.org/occupant#BuildingSimpleState
:BuildingSimpleState rdf:type owl:Class ;
                     rdfs:subClassOf :BuildingState ;
                     rdfs:comment "The building simple state refers to specific status of some building elements that highly impacts in the detection of building anomalies even related with energy or user comfort."@en ;
                     rdfs:label "Building Simple State"@en .


###  http://bemserver.org/occupant#BuildingState
:BuildingState rdf:type owl:Class ;
               rdfs:comment "The building state allows to conextualise and determine the building facilities status. This building state determinaton has been divided into simple building states and complex building states."@en ;
               rdfs:label "Building State"@en .


###  http://bemserver.org/occupant#ComfortState
:ComfortState rdf:type owl:Class ;
              rdfs:subClassOf :BuildingComplexState ;
              rdfs:comment "Comfort state is defined as the ocupant state according to the current building situation."@en ;
              rdfs:label "Comfort State"@en .


###  http://bemserver.org/occupant#Constraints
:Constraints rdf:type owl:Class ;
             rdfs:subClassOf :Event ;
             rdfs:comment "Event that limits the action of something."@en ;
             rdfs:label "Constraints"@en .


###  http://bemserver.org/occupant#Context
:Context rdf:type owl:Class ;
         rdfs:subClassOf :Event ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :isAssignedTo ;
                           owl:someValuesFrom [ rdf:type owl:Class ;
                                                owl:unionOf ( :Indoor
                                                              :Outdoor
                                                            )
                                              ]
                         ] ;
         rdfs:comment "Context in which applies the action"@en ;
         rdfs:label "Context"@en .


###  http://bemserver.org/occupant#DoorClose
:DoorClose rdf:type owl:Class ;
           rdfs:subClassOf :DoorState ;
           owl:disjointWith :DoorOpen ;
           rdfs:comment "Determines that the corresponding door is close"@en ;
           rdfs:label "Door Close"@en .


###  http://bemserver.org/occupant#DoorOpen
:DoorOpen rdf:type owl:Class ;
          rdfs:subClassOf :DoorState ;
          rdfs:comment "Determines that the corresponding door is open"@en ;
          rdfs:label "Door Open"@en .


###  http://bemserver.org/occupant#DoorState
:DoorState rdf:type owl:Class ;
           rdfs:subClassOf :BuildingSimpleState ;
           rdfs:comment "Determines that the corresponding state of the Door (Open or Close)"@en ;
           rdfs:label "Door State"@en .


###  http://bemserver.org/occupant#Driver
:Driver rdf:type owl:Class ;
        rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                 owl:onProperty :hasFunction ;
                                                 owl:someValuesFrom :Function
                                               ]
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :hasIndicator ;
                                                 owl:someValuesFrom :Indicator
                                               ]
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :hasModelResult ;
                                                 owl:someValuesFrom :ModelResult
                                               ]
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :hasStatus ;
                                                 owl:someValuesFrom :Status
                                               ]
                                             ) ;
                          rdf:type owl:Class
                        ] ;
        rdfs:comment "Drivers represent the environmental factors from the outside world that stimulate occupants in their inside world to fulfill a physical, physiological or psychological need"@en ;
        rdfs:label "Driver"@en .


###  http://bemserver.org/occupant#EnergyFrugal
:EnergyFrugal rdf:type owl:Class ;
              rdfs:subClassOf :OccupantAttitude ;
              rdfs:comment "Is the quality of being frugal, sparing, thrifty, prudent or economical in the consumption of consumable resources such as food, time or money, and avoiding waste, lavishness or extravagance"@en ;
              rdfs:label "Energy Frugal"@en .


###  http://bemserver.org/occupant#EnergyIndiferent
:EnergyIndiferent rdf:type owl:Class ;
                  rdfs:subClassOf :OccupantAttitude ;
                  rdfs:comment "Occupant trend to have no preference or being uninterested about building energy consumption and efficiency."@en ;
                  rdfs:label "Energy Indiferent"@en .


###  http://bemserver.org/occupant#EnergyProfligate
:EnergyProfligate rdf:type owl:Class ;
                  rdfs:subClassOf :OccupantAttitude ;
                  rdfs:comment "Occupant that is recklessly, extravagant or wasteful in the use of resources."@en ;
                  rdfs:label "Energy Profligate"@en .


###  http://bemserver.org/occupant#EnergyState
:EnergyState rdf:type owl:Class ;
             rdfs:subClassOf :BuildingComplexState ;
             rdfs:comment "Energy state is the combination of different direct measures or simple state measures to determine energy inefficiencies."@en ;
             rdfs:label "Energy State"@en .


###  http://bemserver.org/occupant#EnvironmentalBuildingFactors
:EnvironmentalBuildingFactors rdf:type owl:Class ;
                              rdfs:comment "Environmental Building Factors refers to the building inddor and outdor fixeed conditions for comfort and discomfort."@en ;
                              rdfs:label "Environmental Building Factors"@en .


###  http://bemserver.org/occupant#EquipementModel
:EquipementModel rdf:type owl:Class ;
                 rdfs:subClassOf :Driver ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :isBasedOn ;
                                   owl:someValuesFrom :EnvironmentalBuildingFactors
                                 ] ;
                 rdfs:comment "Class for measuring the building features and the environmental factors including indoor and outdoor parameters."@en ;
                 rdfs:label "Equipement Model"@en .


###  http://bemserver.org/occupant#Event
:Event rdf:type owl:Class ;
       rdfs:comment "Circumstances that may be driving occupant actions such as waking up, sleeping, leaving for work or returning from lunch"@en ;
       rdfs:label "Event"@en .


###  http://bemserver.org/occupant#EventModel
:EventModel rdf:type owl:Class ;
            rdfs:subClassOf :Driver ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :isBasedOn ;
                              owl:someValuesFrom :Event
                            ] ;
            rdfs:comment "Defined to register the events that can be detected from the sensor readings."@en ;
            rdfs:label "Event Model"@en .


###  http://bemserver.org/occupant#Feedback
:Feedback rdf:type owl:Class ;
          rdfs:subClassOf :Actions ;
          rdfs:comment "In building domain, Feedback is the actor retrofit with information regarding buildn state."@en ;
          rdfs:label "Feedback"@en .


###  http://bemserver.org/occupant#Function
:Function rdf:type owl:Class ;
          rdfs:comment "Is the model function to determine indoor and outdoor environmental building factors."@en ;
          rdfs:label "Function"@en .


###  http://bemserver.org/occupant#Gender
:Gender rdf:type owl:Class ;
        rdfs:comment "The Gender is is the range of characteristics pertaining to, and differentiating between, masculinity and femininity"@en ;
        rdfs:label "Gender"@en .


###  http://bemserver.org/occupant#IAQ
:IAQ rdf:type owl:Class ;
     rdfs:subClassOf :Needs ;
     rdfs:comment "The IAQ corresponds to the types of measurements to determine the inddor environmental health."@en ;
     rdfs:label "IAQ"@en .


###  http://bemserver.org/occupant#ISOAdaptiveComfort
:ISOAdaptiveComfort rdf:type owl:Class ;
                    rdfs:subClassOf :ThermalComfort ;
                    rdfs:comment "This International Standard covering the evaluation of moderate thermal environments was developed in parallel with the revised ASHRAE) standard 55 and is one of a series of ISO documents specifying methods for the measurement and evaluation of the moderate and extreme thermal environments to which human beings are exposed (ISO 7243, ISO 7933 and ISO/TR 11079, all three dealing with extreme environmental conditions, are others in the series)."@en ;
                    rdfs:label "ISO Adaptive Comfort"@en .


###  http://bemserver.org/occupant#Inaction
:Inaction rdf:type owl:Class ;
          rdfs:subClassOf :Actions ;
          rdfs:comment "Refers to the performance of any action in the building."@en ;
          rdfs:label "Inaction"@en .


###  http://bemserver.org/occupant#Indicator
:Indicator rdf:type owl:Class ;
           rdfs:comment "Complementing the Driver information, the indicator refers to the optimal measure that indicates the state or level of something."@en ;
           rdfs:label "Indicator"@en .


###  http://bemserver.org/occupant#Indoor
:Indoor rdf:type owl:Class ;

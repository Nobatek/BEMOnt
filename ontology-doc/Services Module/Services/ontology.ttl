@prefix : <http://bemserver.org/services#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://bemserver.org/services> .

<http://bemserver.org/services> rdf:type owl:Ontology ;
                                 owl:versionIRI <http://bemserver.org/services/0.1> ;
                                 <http://purl.org/dc/terms/creator> "Lara Kallab" ,
                                                                    "Nathalie Charbel" ,
                                                                    "Pierre Bourreau" ;
                                 <http://purl.org/dc/terms/description> "This ontology is used to describe services that are client of BEMServer. Services are programs that can read/get data from BEMServer to perform specific operations, and/or push/write information on BEMServer. In some cases, services can also be seen as specific cases of sensors, producing timeseries to BEMServer. The ontology can be used to generate a catalogue of services that are installed on BEMServer."@en ;
                                 <http://purl.org/dc/terms/title> "BEMServer services ontology"@en ;
                                 owl:versionInfo "v0.1" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#description
rdfs:description rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#desription
rdfs:desription rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://bemserver.org/services#hasOutput
:hasOutput rdf:type owl:ObjectProperty ;
           owl:inverseOf :model .


###  http://bemserver.org/services#installedOn
:installedOn rdf:type owl:ObjectProperty .


###  http://bemserver.org/services#localization
:localization rdf:type owl:ObjectProperty .


###  http://bemserver.org/services#model
:model rdf:type owl:ObjectProperty .


###  http://bemserver.org/services#parameters
:parameters rdf:type owl:ObjectProperty .


###  http://bemserver.org/services#partOfService
:partOfService rdf:type owl:ObjectProperty ;
               owl:inverseOf :usesModel .


###  http://bemserver.org/services#service
:service rdf:type owl:ObjectProperty .


###  http://bemserver.org/services#usesModel
:usesModel rdf:type owl:ObjectProperty .


###  http://bemserver.org/services#valueType
:valueType rdf:type owl:ObjectProperty .


###  http://bemserver.org/services#valueUnit
:valueUnit rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://bemserver.org/services#description
:description rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf <http://www.w3.org/ns/hydra/core#description> ;
             rdfs:description "A description of the service. Must be explicit and clear enough to allow other service developers to understand the suitability of the service for their own use." .


###  http://bemserver.org/services#externalID
:externalID rdf:type owl:DatatypeProperty ;
            rdfs:description "The external Identifier of time series generated by the service if needed (i.e. in case the time series storage uses IDs different from the IDs used to describe time series) " .


###  http://bemserver.org/services#hasFrontend
:hasFrontend rdf:type owl:DatatypeProperty ;
             rdfs:description "A boolean o whether the service has a frontend/user interface or not. This can be used to filter services to present to end users. " .


###  http://bemserver.org/services#id
:id rdf:type owl:DatatypeProperty ;
    rdfs:range xsd:string .


###  http://bemserver.org/services#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf <http://www.w3.org/ns/hydra/core#title> .


###  http://bemserver.org/services#param_name
:param_name rdf:type owl:DatatypeProperty .


###  http://bemserver.org/services#param_value
:param_value rdf:type owl:DatatypeProperty .


###  http://bemserver.org/services#sampling
:sampling rdf:type owl:DatatypeProperty .


###  http://www.w3.org/ns/hydra/core#description
<http://www.w3.org/ns/hydra/core#description> rdf:type owl:DatatypeProperty .


###  http://www.w3.org/ns/hydra/core#entrypoint
<http://www.w3.org/ns/hydra/core#entrypoint> rdf:type owl:DatatypeProperty .


###  http://www.w3.org/ns/hydra/core#title
<http://www.w3.org/ns/hydra/core#title> rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://bemserver.org/property#property
<http://bemserver.org/property#property> rdf:type owl:Class .


###  http://bemserver.org/services#ComposedService
:ComposedService rdf:type owl:Class ;
                 rdfs:subClassOf :Service ;
                 owl:disjointWith :SingleService ;
                 rdfs:label "ComposedService" .


###  http://bemserver.org/services#DataCollectionService
:DataCollectionService rdf:type owl:Class ;
                       rdfs:subClassOf :SingleService ;
                       owl:disjointWith :DataComputationService ,
                                        :DataPreProcessingService ;
                       rdfs:description "A data collection service can only collect information. An example is a weather prediction service, or energy price. It cannot make complex computations on the data." .


###  http://bemserver.org/services#DataComputationService
:DataComputationService rdf:type owl:Class ;
                        rdfs:subClassOf :SingleService ;
                        owl:disjointWith :DataPreProcessingService ;
                        rdfs:description "A data computation service is one that performs complex operations, in opposition with a data collection service. It typically takes some inputs from BEMServer and performs operations. Results can be pushed back to BEMServer or not." .


###  http://bemserver.org/services#DataPreProcessingService
:DataPreProcessingService rdf:type owl:Class ;
                          rdfs:subClassOf :SingleService ;
                          rdfs:description "Data preprocessing services are those that are used to transform, accomodate data to ease their exploitation by other services. For instance, a preprocessing service can resample data, perform atomic operations like computing means, sums... on timeseries." .


###  http://bemserver.org/services#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf :Output ;
       owl:disjointWith :TimeSeries ;
       rdfs:description "Events are specific outputs, that are notification of some event happening in the building or on BEMServer itself. For instance, an event can be an alert on energy peaks, faulty HVAC systems..." ;
       rdfs:label "Event" .


###  http://bemserver.org/services#Model
:Model rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :hasOutput ;
                         owl:allValuesFrom :Output
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :parameters ;
                         owl:allValuesFrom :Parameter
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :partOfService ;
                         owl:allValuesFrom :Service
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :description ;
                         owl:someValuesFrom xsd:string
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :id ;
                         owl:someValuesFrom xsd:string
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :name ;
                         owl:someValuesFrom xsd:string
                       ] ;
       rdfs:comment "A model is a particular configuration of service. As such it can be seen as a sub-service; For instance, if the service runs neural networks, it can run over different neural networks models."@en ;
       rdfs:label "Model"@en .


###  http://bemserver.org/services#Output
:Output rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :model ;
                          owl:someValuesFrom :Model
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :service ;
                          owl:allValuesFrom :Service
                        ] ;
        rdfs:desription "The output of a service that pushed data to BEMServer. Outputs can be of different kinds. A service may have many outputs." ;
        rdfs:label "Output" .


###  http://bemserver.org/services#Parameter
:Parameter rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :param_name ;
                             owl:someValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :param_value ;
                             owl:someValuesFrom xsd:string
                           ] ;
           rdfs:description "Parameters of the service or model, presented as a couple (name, value)." ;
           rdfs:label "Input" ,
                      "Parameter" .


###  http://bemserver.org/services#Service
:Service rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :usesModel ;
                           owl:someValuesFrom :Model
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :installedOn ;
                           owl:allValuesFrom <https://w3id.org/bot#Site>
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://www.w3.org/ns/hydra/core#description> ;
                           owl:someValuesFrom xsd:string
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://www.w3.org/ns/hydra/core#entrypoint> ;
                           owl:someValuesFrom xsd:anyURI
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <http://www.w3.org/ns/hydra/core#title> ;
                           owl:someValuesFrom xsd:string
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :id ;
                           owl:allValuesFrom xsd:string
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasFrontend ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:boolean
                         ] ;
         rdfs:comment "A service defined in BEMServer is an external component that is a client of BEMServer. It can get data from BEMServer, or push some new data to it. As such, it can provide one or more functionalities."@en ;
         rdfs:label "Service"@en ,
                    "Service"@fr ,
                    "Web service"@en .


###  http://bemserver.org/services#SingleService
:SingleService rdf:type owl:Class ;
               rdfs:subClassOf :Service ;
               rdfs:description "A single service is a service that is not composed. In a service composition framework, it is the smallest element available." ;
               rdfs:label "SingleService" .


###  http://bemserver.org/services#TimeSeries
:TimeSeries rdf:type owl:Class ;
            rdfs:subClassOf :Output ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :localization ;
                              owl:someValuesFrom <https://w3id.org/bot#Zone>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :valueType ;
                              owl:someValuesFrom <http://bemserver.org/property#property>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :valueUnit ;
                              owl:someValuesFrom <http://qudt.org/schema/qudt#Unit>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :externalID ;
                              owl:someValuesFrom xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :sampling ;
                              owl:someValuesFrom xsd:float
                            ] ;
            rdfs:description "Time series are specific outputs, similar to time series collected from sensors. BEMServer services can themselves be timeseries clients, and/or timeseries producers." ;
            rdfs:label "TimeSeries" .


###  http://qudt.org/schema/qudt#Unit
<http://qudt.org/schema/qudt#Unit> rdf:type owl:Class .


###  https://w3id.org/bot#Site
<https://w3id.org/bot#Site> rdf:type owl:Class .


###  https://w3id.org/bot#Zone
<https://w3id.org/bot#Zone> rdf:type owl:Class .



    <!-- http://bemserver.org/services#Service -->


    <owl:Class rdf:about="http://bemserver.org/services#Service">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#usesModel"/>
                <owl:someValuesFrom rdf:resource="http://bemserver.org/services#Model"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#installedOn"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/bot#Site"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/hydra/core#description"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/hydra/core#entrypoint"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/hydra/core#title"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#id"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#hasFrontend"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A service defined in BEMServer is an external component that is a client of BEMServer. It can get data from BEMServer, or push some new data to it. As such, it can provide one or more functionalities.</rdfs:comment>
        <rdfs:label xml:lang="en">Service</rdfs:label>
        <rdfs:label xml:lang="fr">Service</rdfs:label>
        <rdfs:label xml:lang="en">Web service</rdfs:label>
    </owl:Class>
    


    <!-- http://bemserver.org/services#SingleService -->


    <owl:Class rdf:about="http://bemserver.org/services#SingleService">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/services#Service"/>
        <rdfs:description>A single service is a service that is not composed. In a service composition framework, it is the smallest element available.</rdfs:description>
        <rdfs:label>SingleService</rdfs:label>
    </owl:Class>
    


    <!-- http://bemserver.org/services#TimeSeries -->


    <owl:Class rdf:about="http://bemserver.org/services#TimeSeries">

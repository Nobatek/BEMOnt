    <!-- http://bemserver.org/property#property -->


    <owl:Class rdf:about="http://bemserver.org/property#property"/>
    


    <!-- http://bemserver.org/services#ComposedService -->


    <owl:Class rdf:about="http://bemserver.org/services#ComposedService">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/services#Service"/>
        <owl:disjointWith rdf:resource="http://bemserver.org/services#SingleService"/>
        <rdfs:label>ComposedService</rdfs:label>
    </owl:Class>
    


    <!-- http://bemserver.org/services#DataCollectionService -->


    <owl:Class rdf:about="http://bemserver.org/services#DataCollectionService">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/services#SingleService"/>
        <owl:disjointWith rdf:resource="http://bemserver.org/services#DataComputationService"/>
        <owl:disjointWith rdf:resource="http://bemserver.org/services#DataPreProcessingService"/>
        <rdfs:description>A data collection service can only collect information. An example is a weather prediction service, or energy price. It cannot make complex computations on the data.</rdfs:description>
    </owl:Class>
    


    <!-- http://bemserver.org/services#DataComputationService -->


    <owl:Class rdf:about="http://bemserver.org/services#DataComputationService">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/services#SingleService"/>
        <owl:disjointWith rdf:resource="http://bemserver.org/services#DataPreProcessingService"/>
        <rdfs:description>A data computation service is one that performs complex operations, in opposition with a data collection service. It typically takes some inputs from BEMServer and performs operations. Results can be pushed back to BEMServer or not.</rdfs:description>
    </owl:Class>
    


    <!-- http://bemserver.org/services#DataPreProcessingService -->


    <owl:Class rdf:about="http://bemserver.org/services#DataPreProcessingService">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/services#SingleService"/>
        <rdfs:description>Data preprocessing services are those that are used to transform, accomodate data to ease their exploitation by other services. For instance, a preprocessing service can resample data, perform atomic operations like computing means, sums... on timeseries.</rdfs:description>
    </owl:Class>
    


    <!-- http://bemserver.org/services#Event -->


    <owl:Class rdf:about="http://bemserver.org/services#Event">
        <rdfs:subClassOf rdf:resource="http://bemserver.org/services#Output"/>
        <owl:disjointWith rdf:resource="http://bemserver.org/services#TimeSeries"/>
        <rdfs:description>Events are specific outputs, that are notification of some event happening in the building or on BEMServer itself. For instance, an event can be an alert on energy peaks, faulty HVAC systems...</rdfs:description>
        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


    <!-- http://bemserver.org/services#Model -->


    <owl:Class rdf:about="http://bemserver.org/services#Model">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#hasOutput"/>
                <owl:allValuesFrom rdf:resource="http://bemserver.org/services#Output"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#parameters"/>
                <owl:allValuesFrom rdf:resource="http://bemserver.org/services#Parameter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#partOfService"/>
                <owl:allValuesFrom rdf:resource="http://bemserver.org/services#Service"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#description"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#id"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#name"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A model is a particular configuration of service. As such it can be seen as a sub-service; For instance, if the service runs neural networks, it can run over different neural networks models.</rdfs:comment>
        <rdfs:label xml:lang="en">Model</rdfs:label>
    </owl:Class>
    


    <!-- http://bemserver.org/services#Output -->


    <owl:Class rdf:about="http://bemserver.org/services#Output">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#model"/>
                <owl:someValuesFrom rdf:resource="http://bemserver.org/services#Model"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#service"/>
                <owl:allValuesFrom rdf:resource="http://bemserver.org/services#Service"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:desription>The output of a service that pushed data to BEMServer. Outputs can be of different kinds. A service may have many outputs.</rdfs:desription>
        <rdfs:label>Output</rdfs:label>
    </owl:Class>
    


    <!-- http://bemserver.org/services#Parameter -->


    <owl:Class rdf:about="http://bemserver.org/services#Parameter">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#param_name"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://bemserver.org/services#param_value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:description>Parameters of the service or model, presented as a couple (name, value).</rdfs:description>
        <rdfs:label>Input</rdfs:label>
        <rdfs:label>Parameter</rdfs:label>

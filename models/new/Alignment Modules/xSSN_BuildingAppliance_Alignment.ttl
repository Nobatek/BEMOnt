<http://bemserver.org/sensor/xSSN_BuildingAppliance_Alignment#> rdf:type owl:Ontology , voaf:Vocabulary ;
	dcterms:title "xSSN to BuildingAppliance alignment."@en ;
	dcterms:description """This ontology defines proposed alignments between xSSN and BuildingAppliance ontology."""@en ;
	dcterms:issued "2021-05-26"^^xsd:date ;
    owl:versionInfo "v1.0.0" ;
    owl:versionIRI <http://bemserver.org/sensor/xSSN_SAREF_Alignment-1.0> ;
	dcterms:creator <https://orcid.org/0000-0002-5520-2270> ;
	dcterms:creator "Nathalie Charbel" ;
	owl:imports 	<http://www.bemserver.org/sensor/xssn/#> ,
					<http://bemserver.org/appliance#> .

dcterms:title a owl:AnnotationProperty .
dcterms:description a owl:AnnotationProperty .
dcterms:issued a owl:AnnotationProperty .
dcterms:modified a owl:AnnotationProperty .
dcterms:creator a owl:AnnotationProperty .
dcterms:contributor a owl:AnnotationProperty .
dcterms:license a owl:AnnotationProperty .
vann:preferredNamespacePrefix a owl:AnnotationProperty .
vann:preferredNamespaceUri a owl:AnnotationProperty .
vcard:fn a owl:AnnotationProperty .
schema:name a owl:AnnotationProperty .

<https://orcid.org/0000-0002-5520-2270> a vcard:Individual , schema:Person; 
	vcard:fn    "Nathalie Charbel" ;
	schema:name "Nathalie Charbel" .

#################################################################
#    Class alignment
#################################################################

sosa:FeatureOfInterest owl:equivalentClass saref:FeatureOfInterest .
sosa:Observation rdfs:subClassOf saref:Measurement .
sosa:ObservableProperty owl:equivalentClass saref:Property .
saref:Sensor rdfs:subClassOf sosa:Sensor .
saref4bldg:BuildingDevice rdfs:subClassOf sosa:FeatureOfInterest .

#################################################################
#    Object property alignement
#################################################################

sosa:observes rdfs:subPropertyOf saref:measures .
sosa:madeObervation rdfs:subPropertyOf saref:makesMeasurement .
sosa:isPropertyOf owl:equivalentProperty saref:isPropertyOf .
sosa:hasFeatureOfInterest rdfs:subPropertyOf saref:isMeasurementOf .
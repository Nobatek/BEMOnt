                                  dc:title "Refactored BEMServerProperty ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/contributor
dc:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
terms:license rdf:type owl:AnnotationProperty .


###  http://qudt.org/schema/qudt#code
qudt:code rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://qudt.org/schema/qudt#string1024
qudt:string1024 rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#anySimpleType
xsd:anySimpleType rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://bemserver.org/property#hasOccupantProperty
:hasOccupantProperty rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasPhenomenonProperty .

###  http://bemserver.org/property#hasPhenomenonProperty
:hasPhenomenonProperty rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf ssn:hasProperty ;
					   rdfs:range sml:QuantityValue .

###  http://bemserver.org/property#hasPhysicalProperty
:hasPhysicalProperty rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasPhenomenonProperty .
					   
###  http://bemserver.org/property#capacity
:capacity rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasPhysicalProperty ;
		  sml:quantityKind quantitykind:Capacity ;
          rdfs:comment "capacity represents an observation of the measure of the capacity of an elment in the building."@en ;
          rdfs:label "capacity"@en .

###  http://bemserver.org/property#color
:color rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :hasPhysicalProperty ;
       rdfs:comment "color represents an observation of the value of the color of an element in the building in a specific time."@en ;
       rdfs:label "color"@en .

###  http://bemserver.org/property#conductivity
:conductivity rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasPhysicalProperty ;
			  sml:quantityKind quantitykind:Conductivity ;
              rdfs:comment "conductivity represents an observation of the measure of the conductivity of an element in the building in a specific time."@en ;
              rdfs:label "conductivity"@en .


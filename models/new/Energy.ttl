#################################################################
#    Classes
#################################################################

###  http://bemserver.org/energy#NonRenewableEnergySource
energy:NonRenewableEnergySource rdf:type owl:Class ;
                                owl:equivalentClass [ rdf:type owl:Class ;
                                                      owl:oneOf ( energy:COAL
                                                                  energy:DIESEL
                                                                  energy:FUEL
                                                                  energy:GAS
                                                                  energy:NUCLEAR
                                                                  energy:OIL
                                                                )
                                                    ] .


###  http://bemserver.org/energy#RenewableEnergySource
energy:RenewableEnergySource rdf:type owl:Class ;
                             owl:equivalentClass [ rdf:type owl:Class ;
                                                   owl:oneOf ( energy:BIOMASS
                                                               energy:GEOTHERMAL
                                                               energy:MICROTURBINE
                                                               energy:PHOTOVOLTAIC
                                                               energy:SOLARHYBRID
                                                               energy:SOLARPOWER
                                                               energy:WINDHYBRID
                                                               energy:WINDPOWER
                                                             )
                                                 ] .



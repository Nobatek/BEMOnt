    <!-- http://data.nasa.gov/qudt/owl/qudt#unit -->

    <owl:ObjectProperty rdf:about="http://data.nasa.gov/qudt/owl/qudt#unit">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:label xml:lang="en">unit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.qudt.org/qudt/owl/1.0.0/qudt.owl#hasUnit -->

    <owl:ObjectProperty rdf:about="http://data.qudt.org/qudt/owl/1.0.0/qudt.owl#hasUnit">
        <rdfs:isDefinedBy xml:lang="en">http://data.qudt.org/qudt/owl/1.0.0/qudt.owl#hasUnit</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#acts -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#acts">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:comment xml:lang="en">Relation to define that an iser acto ober certain device changing their behaviour.</rdfs:comment>
        <rdfs:label xml:lang="en">acts</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#assignedTo -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#assignedTo">
        <owl:inverseOf rdf:resource="http://bemserver.org/occupant#isAssignedTo"/>
        <rdfs:comment xml:lang="en">Relation between the context and indoor or outddor factors.</rdfs:comment>
        <rdfs:label xml:lang="en">assigned to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#associatedTo -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#associatedTo">
        <rdfs:comment xml:lang="en">Relation between the context and certain sensor output</rdfs:comment>
        <rdfs:label xml:lang="en">associated to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#changes -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#changes">
        <rdfs:comment xml:lang="en">The relation to represent the result of an action in terms of changing something inside the buildign (system, sensors, etc).</rdfs:comment>
        <rdfs:label xml:lang="en">changes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#concerns -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#concerns">
        <rdfs:comment xml:lang="en">Relation focused on depicting the link between the users and the corresponding feedback</rdfs:comment>
        <rdfs:label xml:lang="en">concerns</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasBehaviour -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasBehaviour">
        <rdfs:comment xml:lang="en">relation focused on indicating that certain user have defined certain behaviour</rdfs:comment>
        <rdfs:label xml:lang="en">has behaviour</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasComfortRequirements -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasComfortRequirements">
        <rdfs:comment xml:lang="en">The relation to represent that a user has certain comfort requirements to be satisfied</rdfs:comment>
        <rdfs:label xml:lang="en">has comfort requirements</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasConstraints -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasConstraints">
        <rdfs:comment xml:lang="en">the relation to represent that an event have a set of contraints</rdfs:comment>
        <rdfs:label xml:lang="en">has constraints</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasContext -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasContext">
        <rdfs:comment xml:lang="en">Indicate the relation between an event and certain context</rdfs:comment>
        <rdfs:label xml:lang="en">has context</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasFunction -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasFunction">
        <rdfs:comment xml:lang="en">Property to define in the Drivers several functions to measure the comfort.</rdfs:comment>
        <rdfs:label xml:lang="en">has Function</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasGender -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasGender">
        <rdfs:comment xml:lang="en">property to associate to the users their corresponding gender</rdfs:comment>
        <rdfs:label xml:lang="en">has gender</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasIndicator -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasIndicator">
        <rdfs:label xml:lang="en">has indicator</rdfs:label>
        <rdfs:label xml:lang="en">property to indicate to the drivers the associations to certain indicators.</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasModelResult -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasModelResult">
        <rdfs:comment xml:lang="en">property to link the drivers with certain model results.</rdfs:comment>
        <rdfs:label xml:lang="en">has model result</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasOccupantAttitude -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasOccupantAttitude">
        <rdfs:domain rdf:resource="http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcOccupant"/>
        <rdfs:range rdf:resource="http://bemserver.org/occupant#OccupantAttitude"/>
        <rdfs:comment xml:lang="en">object property to represent the occupant attitude of the building occupants.</rdfs:comment>
        <rdfs:label xml:lang="en">has occupant attitude</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasOccupantCharacteristics -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasOccupantCharacteristics">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcOccupant"/>
        <rdfs:range rdf:resource="http://bemserver.org/occupant#OccupantCharacteristics"/>
        <rdfs:comment xml:lang="en">property to define the association between users and their daily activities</rdfs:comment>
        <rdfs:label xml:lang="en">has occuant characteristics</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasProbability -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasProbability">
        <rdfs:comment xml:lang="en">property to represent the probability of happen certain event</rdfs:comment>
        <rdfs:label xml:lang="en">has probability</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasRole -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasRole">
        <rdfs:comment xml:lang="en">property to associate the building occupants to certain role</rdfs:comment>
        <rdfs:label xml:lang="en">has role</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasState -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasState">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://bemserver.org/occupant#isStatedBy"/>
        <rdfs:range rdf:resource="http://bemserver.org/occupant#BuildingState"/>
        <rdfs:comment xml:lang="en">property to asign to the building systems certain associated abstract state</rdfs:comment>
        <rdfs:label xml:lang="en">has state</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#hasStatus -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#hasStatus">
        <rdfs:comment xml:lang="en">property to represent the asignment to the driver of certain status</rdfs:comment>
        <rdfs:label xml:lang="en">has status</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#isAssignedTo -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#isAssignedTo">
        <rdfs:comment xml:lang="en">is assigned to</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#isBasedOn -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#isBasedOn">
        <rdfs:label xml:lang="en">is Based on</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#isProvokedBy -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#isProvokedBy">
        <owl:inverseOf rdf:resource="http://bemserver.org/occupant#provokes"/>
        <rdfs:label xml:lang="en">is provoked by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#isStatedBy -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#isStatedBy">
        <rdfs:label xml:lang="en">is stated by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#performsAction -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#performsAction">
        <rdfs:domain rdf:resource="http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcOccupant"/>
        <rdfs:range rdf:resource="http://bemserver.org/occupant#Actions"/>
        <rdfs:label xml:lang="en">perform action</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#prefers -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#prefers">
        <rdfs:comment xml:lang="en">property to link user with modalitites</rdfs:comment>
        <rdfs:label xml:lang="en">prefers</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#provokes -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#provokes">
        <rdfs:label xml:lang="en">provokes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#receives -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#receives">
        <rdfs:comment xml:lang="en">property to represent the action of receiving notification from the system according t certain building anomalies</rdfs:comment>
        <rdfs:label xml:lang="en">receives</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://bemserver.org/occupant#receivesAlerts -->

    <owl:ObjectProperty rdf:about="http://bemserver.org/occupant#receivesAlerts">
        <rdfs:domain rdf:resource="http://www.buildingsmart-tech.org/ifcOWL/IFC2X3_Final#IfcDistributionControlElement"/>
        <rdfs:range rdf:resource="http://bemserver.org/occupant#Alert"/>
        <rdfs:comment xml:lang="en">relation to represent the recepcion of alerts to certain users</rdfs:comment>
        <rdfs:label xml:lang="en">receives alerts</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#hasSubsystem -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/ssnx/ssn#hasSubsystem">
        <rdfs:comment xml:lang="en">Haspart relation between a system and its parts.</rdfs:comment>
        <rdfs:label xml:lang="en">has subsystem</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#observationResult -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/ssnx/ssn#observationResult">
        <rdfs:comment xml:lang="en">Relation linking an Observation (i.e., a description of the context, the Situation, in which the observatioin was made) and a Result, which contains a value representing the value associated with the observed Property.</rdfs:comment>
        <rdfs:label xml:lang="en">observation result</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#observationResultTime -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/ssnx/ssn#observationResultTime">
        <rdfs:comment xml:lang="en">The result time shall describe the time when the result became available, typically when the procedure associated with the observation was completed For some observations this is identical to the phenomenonTime. However, there are important cases where they differ.[O&amp;M]</rdfs:comment>
        <rdfs:label xml:lang="en">observation result time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.oclc.org/NET/ssnx/ssn#observedBy -->

    <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/ssnx/ssn#observedBy">
        <rdfs:label xml:lang="en">observed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/actsOnProperty -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/actsOnProperty">
        <owl:inverseOf rdf:resource="http://www.w3.org/ns/sosa/isActedBy"/>
        <rdfs:comment xml:lang="en">Relation between an Actuation and the property of a FeatureOfInterest it is acting upon.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:label xml:lang="en">acts on property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/hasFeatureOfInterest -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/hasFeatureOfInterest">
        <rdfs:comment xml:lang="en">A relation between an Observation and the entity whose quality was observed, or between an Actuation and the entity whose property was modified, or between an act of Sampling and the entity that was sampled.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:label xml:lang="en">has feature of interest</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/hasResult -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/hasResult">
        <owl:inverseOf rdf:resource="http://www.w3.org/ns/sosa/isResultOf"/>
        <rdfs:comment xml:lang="en">Relation linking an Observation or Actuation or act of Sampling and a Result or Sample.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:label xml:lang="en">has result</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/isActedBy -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/isActedBy">
        <rdfs:comment xml:lang="en">Relation between an ActuatableProperty of a FeatureOfInterest and an Actuation changing its state.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:label xml:lang="en">is acted by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/isResultOf -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/isResultOf">
        <rdfs:comment xml:lang="en">Relation linking a Result to the Observation or Actuation or act of Sampling that created or caused it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:label xml:lang="en">is result of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/madeActuation -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/madeActuation">
        <owl:inverseOf rdf:resource="http://www.w3.org/ns/sosa/madeByActuator"/>
        <rdfs:comment xml:lang="en">Relation between an Actuator and the Actuation it has made.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:label xml:lang="en">made actuation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/madeByActuator -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/madeByActuator">
        <rdfs:comment xml:lang="en">Relation linking an Actuation to the Actuator that made that Actuation.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://www.w3.org/ns/sosa/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">made by actuator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/madeBySensor -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/madeBySensor">
        <owl:inverseOf rdf:resource="http://www.w3.org/ns/sosa/madeObservation"/>
        <rdfs:comment xml:lang="en">Relation between an Observation and the Sensor which made the Observation.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:isDefinedBy xml:lang="en">http://www.w3.org/ns/sosa/madeBySensor</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">made by Sensor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/madeObservation -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/madeObservation">
        <rdfs:comment xml:lang="en">Relation between a Sensor and an Observation made by the Sensor</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:label xml:lang="en">made by observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/observedProperty -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/observedProperty">
        <rdfs:comment xml:lang="en">Relation linking an Observation to the property that was observed. The ObservableProperty should be a property of the FeatureOfInterest (linked by hasFeatureOfInterest) of this Observation.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:isDefinedBy xml:lang="en">http://www.w3.org/ns/sosa/observedProperty</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/ssn/forProperty -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/forProperty">
        <rdfs:comment xml:lang="en">A relation between some aspect of an entity and a Property.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://www.w3.org/ns/ssn/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">for property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/ssn/hasSubSystem -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/hasSubSystem">
        <rdfs:comment xml:lang="en">Relation between a System and its component parts.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://www.w3.org/ns/ssn/hasSubSystem</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasSubSystem</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/ssn/isPropertyOf -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ssn/isPropertyOf">
        <rdfs:isDefinedBy xml:lang="en">http://www.w3.org/ns/ssn/isPropertyOf</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- 

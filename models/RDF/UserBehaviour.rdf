    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://bemserver.org/occupant#age -->

    <owl:DatatypeProperty rdf:about="http://bemserver.org/occupant#age">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="en">Data property to define the age of certain building occupant</rdfs:comment>
        <rdfs:label xml:lang="en">age</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://bemserver.org/occupant#endWorktime -->

    <owl:DatatypeProperty rdf:about="http://bemserver.org/occupant#endWorktime">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
